{
  "tasks": [
    {
      "command": "echo Hello",
      "label": "echo",
      "type": "shell"
    },
    {
      "args": [
        "${file}"
      ],
      "command": "${config:python.pythonPath}",
      "group": "build",
      "label": "Run Python",
      "presentation": {
        "panel": "new",
        "reveal": "always"
      },
      "problemMatcher": {
        "fileLocation": "absolute",
        "pattern": [
          {
            "file": 1,
            "line": 2,
            "message": 0,
            "regexp": "^\\s+File \"(.*?)\", line ([0-9]+)[,]?.+?$"
          },
          {
            "code": 1,
            "regexp": "^\\s+(.+?)$"
          }
        ]
      },
      "type": "shell"
    },
    {
      "command": "${config:python.pythonPath} ${file}",
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "label": "Run File",
      "presentation": {
        "focus": true,
        "panel": "new",
        "reveal": "always"
      },
      "type": "shell"
    },
    {
      "command": "${config:python.pythonPath} -m unittest",
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "label": "unittest",
      "presentation": {
        "focus": true,
        "panel": "new",
        "reveal": "always"
      },
      "type": "shell"
    },
    {
