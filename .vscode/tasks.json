{
  "tasks": [
    {
      "args": [
        "${file}"
      ],
      "command": "${config:python.pythonPath}",
      "group": "build",
      "label": "Run Python",
      "presentation": {
        "panel": "new",
        "reveal": "always"
      },
      "problemMatcher": {
        "fileLocation": "absolute",
        "pattern": [
          {
            "file": 1,
            "line": 2,
            "message": 0,
            "regexp": "^\\s+File \"(.*?)\", line ([0-9]+)[,]?.+?$"
          },
          {
            "code": 1,
            "regexp": "^\\s+(.+?)$"
          }
        ]
      },
      "type": "shell"
    },
    {
      "command": "pytest",
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "label": "pytest",
      "presentation": {
        "focus": true,
        "panel": "new",
        "reveal": "always"
      },
      "type": "shell"
    },
    {
      "command": "${config:python.pythonPath} -m unittest",
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "label": "unittest",
      "presentation": {
        "focus": true,
        "panel": "new",
        "reveal": "always"
      },
      "type": "shell"
    },
    {
      "command": "pipenv shell",
      "dependsOn": [
        "pipenv"
      ],
      "group": "build",
      "isBackground": true,
      "label": "pipenv",
      "type": "shell"
    },
    {
      "command": "${config:python.pythonPath}",
      "dependsOrder": "sequence",
      "group": "build",
      "label": "second try sphinx-build",
      "type": "process",
      "windows": {
        "args": [
          "-m",
          "sphinx-build",
          "-b",
          "html",
          "-o",
          "docs\\build\\html",
          "docs\\source"
        ]
      }
    },
    {
      "args": [
        "-m",
        "autopep8",
        "-i",
        "${file}"
      ],
      "command": "${config:python.pythonPath}",
      "label": "autopep8 file. shell?",
      "type": "shell"
    },
    {
      "command": "${config:python.pythonPath}",
      "group": "none",
      "label": "autopep8 current file. process?",
      "type": "process"
    },
    {
      "args": [
        "-m",
        "yapf",
        "-i",
        "${file}"
      ],
      "command": "${config:python.pythonPath}",
      "group": "none",
      "label": "YAPF current file",
      "type": "process"
    },
    {
      "command": "sphinx-build",
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "label": "sphinx-build with pwsh",
      "options": {
        "shell": {
          "args": [
            "-NoProfile",
            "-NoLogo",
            "-NoExit"
          ],
          "executable": "pwsh"
        }
      },
      "type": "shell",
      "windows": {
        "args": [
          "sphinx-build",
          "-n",
          "-b",
          "html",
          "-o",
          "docs\\build\\html",
          "docs\\source",
          "-d",
          "docs\\build\\doctrees"
        ]
      }
    },
    {
      "label": "npm",
      "path": "jupyter_conf/",
      "script": "install",
      "type": "npm"
    },
    {
      "path": "docs/",
      "problemMatcher": [],
      "script": "install",
      "type": "npm"
    }
  ],
  "version": "2.0.0"
}