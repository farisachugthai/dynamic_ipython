      "type": "shell"
    },
    {
      "command": "${config:python.pythonPath}",
      "group": "none",
      "label": "autopep8 current file. process?",
      "type": "process"
    },
    {
      "args": [
        "-m",
        "yapf",
        "-i",
        "${file}"
      ],
      "command": "${config:python.pythonPath}",
      "group": "none",
      "label": "YAPF current file",
      "type": "process"
    },
    {
      "command": "sphinx-build",
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "label": "sphinx-build with pwsh",
      "options": {
        "shell": {
          "args": [
            "-NoProfile",
            "-NoLogo",
            "-NoExit"
          ],
          "executable": "pwsh"
        }
      },
      "type": "shell",
