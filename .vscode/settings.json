{
  "debug.enableAllHovers": true,
  "debug.inlineValues": true,
  "editor.fontFamily": "'Fira Code', Hack, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.mouseWheelZoom": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "editor.stablePeek": true,
  "editor.suggest.localityBonus": true,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.ipy": "python3"
  },
  "files.autoGuessEncoding": true,
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.autoStash": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.rebaseWhenSync": true,
  "git.showPushSuccessNotification": true,
  "json.trace.server": "messages",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.enableScriptExplorer": true,
  "restructuredtext.confPath": "${workspaceFolder}\\docs\\Source",
  "restructuredtext.languageServer.trace.server": "messages",
  "restructuredtext.updateDelay": 500,
  "python.autoComplete.addBrackets": true,
  "python.condaPath": "C:\\tools\\miniconda3\\Scripts\\conda.exe",
  "python.dataScience.exportWithOutputEnabled": true,
  "python.dataScience.jupyterServerURI": "local",
  "python.dataScience.sendSelectionToInteractiveWindow": true,
  "python.formatting.provider": "autopep8",
  "python.jediPath": "jedi",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.pythonPath": ".venv\\Scripts\\python.exe",
  "python.terminal.executeInFileDir": true,
  "python.testing.nosetestArgs": [
    "test"
  ],
  "python.testing.nosetestsEnabled": false,
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./test",
    "-p",
    "test*.py"
  ],
  "python.testing.unittestEnabled": false,
  "restructuredtext.builtDocumentationPath": "${workspaceFolder}/docs/build",
  "restructuredtext.experimental": true,
  "restructuredtext.styles": [
    "${workspaceFolder}\\docs\\_static\\pyramid.css"
  ],
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.experimentalRefreshOnResume": true,
  "vim.camelCaseMotion.enable": true,
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.foldfix": true,
  "vim.highlightedyank.enable": true,
  "vim.history": 10000,
  "vim.leader": " ",
  "vim.replaceWithRegister": true,
  "vim.smartcase": true,
  "vim.sneak": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.statusBarColorControl": true,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "vsicons.presets.jsOfficial": true,
  "window.closeWhenEmpty": true,
  "workbench.commandPalette.preserveInput": true,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.enableExperiments": false,
  "workbench.quickOpen.preserveInput": true,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.settings.openDefaultKeybindings": true,
  "workbench.settings.useSplitJSON": true,
  "python.testing.pytestEnabled": true
}
