{
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontFamily": "'DejaVu Sans Mono'",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.mouseWheelZoom": true,
  "editor.smoothScrolling": true,
  "editor.stablePeek": true,
  "editor.suggest.localityBonus": true,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "files.autoGuessEncoding": true,
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autoStash": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.rebaseWhenSync": true,
  "npm.enableScriptExplorer": true,
  "python.analysis.cachingLevel": "Library",
  "python.analysis.diagnosticPublishDelay": 300,
  "python.analysis.downloadChannel": "daily",
  "python.analysis.logLevel": "Error",
  "python.autoComplete.addBrackets": true,
  "python.dataScience.changeDirOnImportExport": true,
  "python.dataScience.debugJustMyCode": false,
  "python.dataScience.enableGather": true,
  "python.dataScience.exportWithOutputEnabled": true,
  "python.dataScience.jupyterServerURI": "local",
  "python.jediEnabled": false,
  "python.linting.enabled": false,
  "python.linting.flake8CategorySeverity.F": "Information",
  "python.linting.flake8Enabled": true,
  "python.linting.pydocstyleEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.pipenvPath": "pipenv",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.terminal.executeInFileDir": true,
  "python.testing.pytestEnabled": true,
  "restructuredtext.builtDocumentationPath": "${workspaceFolder}/docs/build",
  "restructuredtext.experimental": true,
  "restructuredtext.styles": [
    "${workspaceFolder}\\docs\\_static\\pyramid.css"
  ],
  "restructuredtext.updateDelay": 500,
  "python.terminal.launchArgs": [],
  "python.formatting.provider": "black",
  // "python.pythonPath": "C:\\Users\\fac\\.virtualenvs\\dynamic_ipython-BYhWmG8z\\Scripts\\python.exe",
  "python.pythonPath": "C:\\Users\\fac\\.virtualenvs\\dynamic_ipython-BYhWmG8z\\Scripts\\python.exe",
  "python.testing.unittestEnabled": false,
  "python.testing.nosetestsEnabled": false,
  "restructuredtext.confPath": "${workspaceFolder}\\source",
  "python.venvPath": "~/.virtualenvs",
  "python.dataScience.themeMatplotlibPlots": true
}