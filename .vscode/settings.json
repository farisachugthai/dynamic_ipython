{
  "editor.fontFamily": "'Fira Code', Hack, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.mouseWheelZoom": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "editor.stablePeek": true,
  "editor.suggest.localityBonus": true,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "files.autoGuessEncoding": true,
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autoStash": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.rebaseWhenSync": true,
  "npm.enableScriptExplorer": true,
  "restructuredtext.updateDelay": 500,
  "python.analysis.cachingLevel": "Library",
  "python.autoComplete.addBrackets": true,
  "python.dataScience.changeDirOnImportExport": true,
  "python.dataScience.debugJustMyCode": false,
  "python.dataScience.enableGather": true,
  "python.dataScience.exportWithOutputEnabled": true,
  "python.dataScience.jupyterServerURI": "local",
  "python.dataScience.sendSelectionToInteractiveWindow": true,
  "python.jediEnabled": false,
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8CategorySeverity.F": "Information",
  "python.linting.pydocstyleEnabled": true,
  // "python.pythonPath": "C:\\Users\\fac\\.virtualenvs\\dynamic_ipython-BYhWmG8z\\Scripts\\python.exe",
  // "python.pythonPath": "/home/farbuntu/.local/share/virtualenvs/dynamic_ipython-SU0AiBHn/bin/python",
  // "python.pythonPath": "C:\\Python38\\python.exe",
  "python.terminal.executeInFileDir": true,
  "python.testing.nosetestArgs": [
    "test"
  ],
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./test",
    "-p",
    "test*.py"
  ],
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": true,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "restructuredtext.builtDocumentationPath": "${workspaceFolder}/docs/build",
  "restructuredtext.confPath": "${workspaceFolder}/docs/source",
  "restructuredtext.confPath": "${workspaceFolder}\\docs\\source",
  "restructuredtext.confPath": "${workspaceFolder}\\docs\\source",
  "restructuredtext.experimental": true,
  "restructuredtext.styles": [
    "${workspaceFolder}\\docs\\_static\\pyramid.css"
  ],
}
